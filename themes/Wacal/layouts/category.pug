include /mixins/script.pug
include /mixins/type_script.pug
doctype html
html 
    head
        +meta(user.note,user.keyword||'')
        +gbase()
        title= __title__(i18n('Categories'),user.name)
    body.new(OCP='OVERRIDE')
        +gnome_header()
        .intro#intro.tac
            h1= i18n('Categories')
        main.content#content
            section
                ul.intros
                    each item in cycling.value
                        +intro_li(item.value)
            +gnome_page_navigator(cycling.total,cycling.now,'category')
        +gnome_footer()
        include /components/searchPanel.pug