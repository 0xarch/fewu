{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#modules": [
                "./_modules"
            ],
            "#util/*": [
                "./_src/util/*.mjs",
                "./_src/util/*.mts"
            ],
            "#lib/*": [
                "./_src/lib/*.mts"
            ]
        }
    },
    "exclude": [
        "_assets",
        "_doc",
        "_themes",
        "_dist"
    ]
}