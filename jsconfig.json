{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#core/*": [
                "./_src/v2/core/*.js"
            ],
            "#struct": [
                "./_src/v2/core/bridge.class.js"
            ],
            "#class/*": [
                "./_src/v2/class/*.mjs"
            ],
            "#modules": [
                "./_modules"
            ],
            "#util/*": [
                "./_src/util/*.mjs",
                "./_src/util/*.mts"
            ],
            "#util-ts/*": [
                "./_src/util/ts/*.mts"
            ],
            "#database": [
                "./_src/v2/database/main.mjs"
            ],
            "#common/*": [
                "./_src/common/*.mjs"
            ],
            "#lib/*": [
                "./_src/lib/*.mts"
            ]
        }
    },
    "exclude": [
        "_assets",
        "_doc",
        "_themes",
        "_dist"
    ]
}